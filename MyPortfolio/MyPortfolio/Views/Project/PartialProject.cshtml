<section class="w3l-services">
    <div class="w3l-achievements py-5" id="services">
        <div class="container py-lg-5">
            @*<h5 class="title-small text-center">Sertifika & Kurs</h5>*@
            <h3 class="title-big text-center mb-sm-5 mb-4">PROJELER</h3>
            <div class="row">
                @foreach (var item in Model)
                {
                    <div class="col-lg-6 item">
                        <div class="card">
                            <div class="box-wrap">
                                <div class="icon">
                                    <span class="@item.ProjectID"></span>
                                </div>
                                <h4>@item.Title</h4>
                                <p>
                                    @item.Description
                                </p>
                                <br />
                                <p style="font-size:15px;">
                                    Proje Tarihi: @item.Tarih.ToString("dd.MM.yyyy")
                                </p>
                            </div>
                        </div>
                    </div>
                }

            </div>
        </div>
    </div>
</section>

